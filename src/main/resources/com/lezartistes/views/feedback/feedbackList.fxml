<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">

   <children>
      <Label layoutX="134.0" layoutY="64.0" text="All of the feedbacks">
         <font>
            <Font size="38.0" />
         </font></Label>
      <ImageView fitHeight="81.0" fitWidth="80.0" layoutX="506.0" layoutY="320.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../asset/logo.png" />
         </image></ImageView>
      <ListView fx:id="feedbacksList" layoutX="95.0" layoutY="131.0" prefHeight="143.0" prefWidth="410.0" />
      <Button layoutX="110.0" layoutY="312.0" mnemonicParsing="false" onMouseClicked="#addFeedback" text="Add a new feedback" />
      <Button fx:id="deleteButton" cancelButton="true" layoutX="321.0" layoutY="312.0" mnemonicParsing="false" onMouseClicked="#deleteFeedback" text="Delete the selected feedback" />
      <Label fx:id="error" layoutX="183.0" layoutY="274.0" prefHeight="37.0" prefWidth="233.0" textFill="RED" />
   </children>
</AnchorPane>
