<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <SplitPane dividerPositions="0.2993311036789298" prefHeight="91.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <items>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <ImageView fitHeight="152.0" fitWidth="157.0" layoutX="12.0" layoutY="-22.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../asset/logo.png" />
                     </image></ImageView>
               </children>
            </Pane>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <ToolBar layoutX="32.0" layoutY="24.0">
                     <items>
                        <Button mnemonicParsing="false" onAction="#redirectToHome" text="Home">
                           <font>
                              <Font name="Lucida Calligraphy Italic" size="14.0" />
                           </font></Button>
                        <Button mnemonicParsing="false" text="Account">
                           <font>
                              <Font name="Lucida Calligraphy Italic" size="14.0" />
                           </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#redirectToCompanyList" text="Company">
                           <font>
                              <Font name="Lucida Calligraphy Italic" size="14.0" />
                           </font>
                        </Button>
                        <Button mnemonicParsing="false" text="Help ?">
                           <font>
                              <Font name="Lucida Calligraphy Italic" size="14.0" />
                           </font>
                        </Button>
                     </items>
                  </ToolBar>
               </children></Pane>
         </items>
      </SplitPane>
   </top>
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="135.0" layoutY="20.0" text="All of the feedbacks">
               <font>
                  <Font size="38.0" />
               </font>
            </Label>
            <ListView fx:id="feedbacksList" layoutX="101.0" layoutY="83.0" prefHeight="143.0" prefWidth="399.0" />
            <Button layoutX="111.0" layoutY="240.0" mnemonicParsing="false" onMouseClicked="#addFeedback" text="Add a new feedback" />
            <Button fx:id="deleteButton" cancelButton="true" layoutX="322.0" layoutY="240.0" mnemonicParsing="false" onMouseClicked="#deleteFeedback" text="Delete the selected feedback" />
            <Label fx:id="error" layoutX="183.0" layoutY="274.0" prefHeight="37.0" prefWidth="233.0" textFill="RED" />
         </children>
      </Pane>
   </center>
</BorderPane>
