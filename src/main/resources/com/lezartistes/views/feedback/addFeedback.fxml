<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <SplitPane dividerPositions="0.2993311036789298" prefHeight="91.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <items>
                <Pane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <ImageView fitHeight="152.0" fitWidth="157.0" layoutX="12.0" layoutY="-22.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../asset/logo.png" />
                            </image></ImageView>
                    </children>
                </Pane>
                <Pane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <ToolBar layoutX="32.0" layoutY="24.0">
                            <items>
                                <Button mnemonicParsing="false" onAction="#redirectToHome" text="Home">
                                    <font>
                                        <Font name="Lucida Calligraphy Italic" size="14.0" />
                                    </font></Button>
                                <Button mnemonicParsing="false" text="Account">
                                    <font>
                                        <Font name="Lucida Calligraphy Italic" size="14.0" />
                                    </font>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#redirectToCompanyList" text="Company">
                                    <font>
                                        <Font name="Lucida Calligraphy Italic" size="14.0" />
                                    </font>
                                </Button>
                                <Button mnemonicParsing="false" text="Help ?">
                                    <font>
                                        <Font name="Lucida Calligraphy Italic" size="14.0" />
                                    </font>
                                </Button>
                            </items>
                        </ToolBar>
                    </children></Pane>
            </items>
        </SplitPane>
    </top>
    <center>
        <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">

            <children>
            <Label contentDisplay="CENTER" layoutX="135.0" layoutY="20.0" text="Add a new feedback">
                <font>
                    <Font name="System" size="38.0" />
                </font>
            </Label>

            <ImageView fitHeight="25.0" fitWidth="38.0" layoutX="100.0" layoutY="35.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../../asset/pen.png" />
                </image>
            </ImageView>

            <Pane layoutX="142.0" layoutY="45.0" prefHeight="250.0" prefWidth="300.0">
                <children>
                    <Label layoutX="32.0" layoutY="54.0" text="Rating :">
                        <font>
                            <Font name="System Bold" size="12.0" />
                        </font>
                    </Label>
                    <Label layoutX="14.0" layoutY="86.0" text="Comment :">
                        <font>
                            <Font name="System Bold" size="12.0" />
                        </font>
                    </Label>

                    <Button layoutX="143.0" layoutY="214.0" mnemonicParsing="false" onMouseClicked="#addFeedback" text="Add" />
                    <Label id="rating" layoutX="143.0" layoutY="38.0" prefHeight="17.0" prefWidth="80.0" />
                    <Label id="comment" layoutX="143.0" layoutY="62.0" prefHeight="17.0" prefWidth="80.0" />
                    <TextField id="rating" fx:id="rating" layoutX="94.0" layoutY="50.0" prefHeight="25.0" prefWidth="176.0" promptText="rate out of 5" />
                    <TextArea id="comment" fx:id="comment" layoutX="94.0" layoutY="86.0" prefHeight="91.0" prefWidth="176.0" />
                    <Label fx:id="error" layoutX="45.0" layoutY="177.0" prefHeight="37.0" prefWidth="233.0" textFill="RED" />
                </children>
            </Pane>
        </children>
        </Pane>
    </center>
</BorderPane>
